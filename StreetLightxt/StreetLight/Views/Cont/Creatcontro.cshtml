@model StreetLight.ViewModels.cont.CreatControViewModel
@{
    ViewData["Title"] = "Creatcellect";
}

<form enctype="multipart/form-data" method="post" asp-controller="Cont" asp-action="Creatcontro" class="mt-3">

    <div asp-validation-summary="All" class="text-danger"></div>


    <div class="form-group row">
        <label asp-for="Name" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Name" class="form-control " placeholder="请输入控制器名称" />
        </div>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>


    <div class="form-group row">
        <label asp-for="Conwell" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Conwell" class="form-control " placeholder="请输入控制器状况" />
        </div>

        <span asp-validation-for="Conwell" class="text-danger"></span>
    </div>

    <div class="form-group row">
        <label asp-for="Consour" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Consour" class="form-control " placeholder="请输入生产厂家" />
        </div>

        <span asp-validation-for="Consour" class="text-danger"></span>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">

            <button type="submit" class="btn btn-primary">添加控制器</button>

        </div>
    </div>





</form>

@model StreetLight.ViewModels.sened.CreatSensorViewModel
@{
    ViewData["Title"] = "Creatcellect";
}

<form enctype="multipart/form-data" method="post" asp-controller="Sensor" asp-action="Creatsensor" class="mt-3">

    <div asp-validation-summary="All" class="text-danger"></div>


    <div class="form-group row">
        <label asp-for="Name" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Name" class="form-control " placeholder="请输入传感器名称" />
        </div>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>


    <div class="form-group row">
        <label asp-for="Senwell" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Senwell" class="form-control " placeholder="请输入传感器状况" />
        </div>

        <span asp-validation-for="Senwell" class="text-danger"></span>
    </div>

    <div class="form-group row">
        <label asp-for="Sensour" class="col-sm-2 col-form-label"></label>

        <div class="col-sm-10">
            <input asp-for="Sensour" class="form-control " placeholder="请输入生产厂家" />
        </div>

        <span asp-validation-for="Sensour" class="text-danger"></span>
    </div>

    <div class="form-group row">
        <div class="col-sm-10">

            <button type="submit" class="btn btn-primary">添加传感器</button>

        </div>
    </div>





</form>

@model StreetLight.ViewModels.cell.CellectViewModel
@{
    ViewData["Title"] = "Index";
}

<table class="table table-hover table-bordered">
    <thread class="thead-dark">
        <tr class="thead-dark">
           
            <th>名称</th>

            <th>设备状况</th>
            <th>生产厂商</th>
            <th>删除采集器</th>

        </tr>
    </thread>

    <tbody>
        @foreach (var cellect in Model.Cellects)
        {


            <tr>
                
                <td>
                    @cellect.Name
                </td>

                <td>

                    @cellect.Cellectwell
                </td>
                <td>
                    @cellect.Cellectsour
                </td>

                <td class="table-danger">
                    <a asp-controller="Cellect" asp-action="Delete" asp-route-id="@cellect.Id"
                       onclick="javascript: return fun();">删除采集器</a>

                </td>

            </tr>

        }

    </tbody>

</table>
<a asp-controller="Cellect" asp-action="Creatcellect" class="btn btn-outline-primary">添加采集器</a>
<a asp-controller="Main" asp-action="Index" class="btn btn-secondary">返回</a>
<script type="text/javascript">
    function fun() {
        var msg = "您真的要删除吗？\n\n请确认！";

        if (confirm(msg) == true) {

            return true;
        }
        else {
            return false;
        }

    }

</script>

@model StreetLight.ViewModels.cell.CreatCellectViewModel
@{
    ViewData["Title"] = "Creatcellect";
}

    <form enctype="multipart/form-data" method="post" asp-controller="Cellect" asp-action="Creatcellect" class="mt-3">

        <div asp-validation-summary="All" class="text-danger"></div>


        <div class="form-group row">
            <label asp-for="Name" class="col-sm-2 col-form-label"></label>

            <div class="col-sm-10">
                <input asp-for="Name" class="form-control " placeholder="请输入采集器名称" />
            </div>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>


        <div class="form-group row">
            <label asp-for="Cellectwell" class="col-sm-2 col-form-label"></label>

            <div class="col-sm-10">
                <input asp-for="Cellectwell" class="form-control " placeholder="请输入采集器状况" />
            </div>

            <span asp-validation-for="Cellectwell" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <label asp-for="Cellectsour" class="col-sm-2 col-form-label"></label>

            <div class="col-sm-10">
                <input asp-for="Cellectsour" class="form-control " placeholder="请输入生产厂家" />
            </div>

            <span asp-validation-for="Cellectsour" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-sm-10">

                <button type="submit" class="btn btn-primary">添加采集器</button>

            </div>
        </div>





    </form>


